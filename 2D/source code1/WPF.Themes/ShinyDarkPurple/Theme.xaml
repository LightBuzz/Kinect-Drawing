                            Height="16"
                            Margin="1"
                            x:Name="VerticalSmallDecrease"
                            Grid.Row="0"
                            Interval="50" 
                            Command="ScrollBar.LineUpCommand"/>

                            <Track Grid.Row="1" x:Name="PART_Track" Orientation="Vertical" IsDirectionReversed="true">
